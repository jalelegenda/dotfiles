alias ls='ls --color=auto --group-directories-first' 
alias status='sudo sv status /var/service/*'
alias logs='sudo less /var/log/socklog/everything/current'
alias restart='sudo sv restart'
alias start='sudo sv start'
alias stop='sudo sv stop'
alias conf_nvim='sudoedit /etc/xdg/nvim/init.vim'
alias nvims='conf_nvim'
alias dockerstop='docker stop $(docker ps -q)'
alias psg='ps aux | grep -i'
alias se='sudoedit'
alias brc='. ~/.bashrc'
alias f='fzf'

